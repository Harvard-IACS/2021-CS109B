    "from scipy.interpolate import splev, splrep\n",
    "\n",
    "x = np.linspace(0, 10, 10)\n",
    "y = np.sin(x)\n",
    "# (t,c,k) is a tuple containing the vector of knots, coefficients, degree of the spline\n",
    "t,c,k = splrep(x, y) \n",
    "x2 = np.linspace(0, 10, 200)\n",
    "y2 = BSpline(t,c,k)\n",
    "plt.plot(x, y, 'o', x2, y2(x2))\n",
    "plt.show()"
   ]
