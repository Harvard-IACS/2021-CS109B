    "x = np.array([1.,0.,-1.5,0.,-1.5,0.])\n",
    "y = np.array([1.5,1.,2.5,3,4,5])\n",
    "t = np.linspace(0,5,6)\n",
    "f = interp1d(t,x,kind='cubic')\n",
    "g = interp1d(t,y,kind='cubic')\n",
    "tplot = np.linspace(0,5,200)\n",
    "plt.plot(x,y, '*', f(tplot), g(tplot));"
